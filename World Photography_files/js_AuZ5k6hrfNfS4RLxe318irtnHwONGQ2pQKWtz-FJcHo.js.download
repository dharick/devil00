(function ($) {

  // disable contextmenu on images
  $('body').on('contextmenu', 'img', function (event) { return false; });

  function isMobile() {
    try{ document.createEvent("TouchEvent"); return true; }
    catch(e){ return false; }
  }

  Drupal.behaviors.worldPhotoStickyHeader = {
    attach: function(context, settings) {
      var header = $('.not-logged-in header[role="navigation"]');
      var main_menu_block = $('#block-menu-block-1');
      var large_header_height = null;
      var fixed_header_height = null;

      // Only get the header height when everything has loaded.
      $(window).on('load', function () {
        large_header_height = header.outerHeight();
      });

      // Don't do this if we're on mobile or in the gallery, especially if we're on the mobile gallery.
      if (!isMobile() && !$('body').hasClass('node-type-gallery')) {
        $(window).scroll(function () {
          var scroll_position = $(document).scrollTop();

          if (header.hasClass('fixed')) {
            if (scroll_position === 0 || scroll_position < (large_header_height - fixed_header_height)) {
              // Switch to the large menu and remove body padding
              header.removeClass('fixed');
              $('body').css('margin-top', 0);

              // Return the main menu block back to the header bottom
              main_menu_block.appendTo('.region--header-bottom');
            }
          } else {
            if ($('body').hasClass('not-logged-in')) {
              if (scroll_position > large_header_height) {
                // Switch to the fixed menu and pad body
                header.addClass('fixed');
                $('body').css('margin-top', large_header_height);

                // Remember the height of the fixed menu
                fixed_header_height = header.outerHeight();

                // Move the main menu into the container
                main_menu_block.insertAfter('#block-search-form');
              }
            }
          }
        });
      }
    }
  }

  Drupal.behaviors.worldPhotoParagraphAccordion = {
    attach: function(context) {

      if ( $('.paragraphs-item-accordion').length > 0 ) {

        $('.js-accordion-trigger').on('click', function(e){
          $(this).siblings('.description').slideToggle('slow');
          $(this).toggleClass('is-expanded');
          e.preventDefault();
        });

        // on page load open the first .js-accordion-trigger and trigger a click to open
        // commented out as per the request of the client
        // $('.js-accordion-trigger:first').trigger('click');
      }

    }
  }

  Drupal.behaviors.worldPhotoShowGalleryBadge = {
    attach: function(context) {

      if ( $('.gallery-badge .onpageload').length > 0 ) {
        $('.gallery-badge .onpageload').fadeIn(750);
      }

    }
  }

  Drupal.behaviors.worldPhotoFixElements = {
    attach: function(context) {

      if (!isMobile()) {
        var height_siblings = 0;
        $(".page.container.content").nextAll('div,footer').each(function(){
          height_siblings += $(this).outerHeight(true);
        });

        // lockfixed jQuery plugin
        // https://github.com/ymschaap/jquery-lockfixed
        if ( $('.not-front .region--off-canvas-left').length > 0 ) {
          $.lockfixed(".region--off-canvas-left",
            {
              offset: {
                top: 100,
                bottom: 100,
                left: 0
              }
          });
        }

        // competition entry link
        if ( $('.node-type-competition .page.container.content .region--sidebar-second .block--competition-link').length > 0 ) {
          $('.node-type-competition .page.container.content .region--sidebar-second .block--competition-link').removeClass('mobile');
          /* The viewport is greater than 700 pixels wide */
          $.lockfixed(".node-type-competition .page.container.content .region--sidebar-second .block--competition-link", {
              offset: {
                top: 65,
                bottom: height_siblings,
                left: 0
              }
          });       
        }

        // blog flipper navigation
        if ( $('ul.flippy').length > 0 ) {

          /*
          $.lockfixed("ul.flippy",
            {
              offset: {
                top: 200,
                bottom: height_siblings,
                left: 0
              }
          });
          */
        }

      } else {
        // Mobile tweaks to competition link. Fix to bottom of screen.
        if ( $('.node-type-competition .page.container.content .region--sidebar-second .block--competition-link').length > 0 ) {
          $('.node-type-competition .page.container.content .region--sidebar-second .block--competition-link').addClass('mobile');
        }
      }

    }
  }

  Drupal.behaviors.worldPhotoMainNavigationWrapper = {
    attach: function(context) {

      // find .block--navigation-main in DOM
      // find ul.menu li.expanded
      // add wrapper div around ul.menu

      $('.block--navigation .expanded > ul.menu').wrap('<div class="menu-dropdown"></div>')

    }
  }

  Drupal.behaviors.worldPhotoTeamProfiles = {
    attach: function(context) {

      if ($('.block--team-profile').length) {
        $('.block--team-profile .show-more-details').on('click', function () {
          var row = $(this).closest('.views-row');

          if (!row.hasClass('expanded')) {
            $('.triangle', this).text('Hide');

            $('.block--team-profile .views-row').removeClass('expanded'); // Remove the class from all rows

            row.addClass('expanded');
          } else {
            $('.triangle', this).text('Read more');

            row.removeClass('expanded');
          }
        });
      }
    }
  }

  Drupal.behaviors.worldPhotoCountdownTimer = {
    attach: function(context) {
      // add class to footer for extra margin if .block-jquery-countdown-timer exists
      if ( $('.block-jquery-countdown-timer').length > 0 ) {
        $('footer').addClass('extra-margin');
      } else {
        $('footer').removeClass('extra-margin');
      }
    }
  }

  Drupal.behaviors.worldPhotoMainNavigation = {
    attach: function(context) {

      // Are we animating or not
      var ANIMATING = false;

      // Add some links for sub-menu triggers
      $('.block--navigation .expanded > a').append('<a class="js--sub-menu-trigger" href="#"></a>');

      // Sub-menu click handler
      $('.js--sub-menu-trigger').on('click', function(event) {
        event.preventDefault();
        $(this).toggleClass('active').parent().siblings('.menu-dropdown').slideToggle(200);
      });

      // User taps burger, navigation region slide toggles
      $('.js--burger-trigger').on('click', function(event) {
        event.preventDefault();

        if (ANIMATING) {
          return;
        }

        ANIMATING = true;

        $('.site-header').toggleClass('header--active');

        $('.region--navigation').slideToggle({
          duration: 200,
          start: function() {
            $(this).css({
              display: "flex"
            })
          },
          done: function() {
            ANIMATING = false;
          }
        });
      });

      // Making sure that the menu re-appears if the user
      // resizes the browser
      $(window).resize(function(event) {
        if ($('.header--burger-box').is(':hidden') && $(window).width() > 767) {
          $('.region--navigation').removeAttr('style');
          $('.site-header').removeClass('header--active');
        }
      });

      // Hover handler for the expanded menu links
      $('.block--navigation ul.menu li.expanded').once().hover(function() {
        if ($(window).width() > 767) {
          $(this).find('>.menu-dropdown').fadeIn(100);
        }
      }, function() {
        if ($(window).width() > 767) {
          $(this).find('>.menu-dropdown').fadeOut(100);
        }
      });
    }
  }

